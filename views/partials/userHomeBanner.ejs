  <!--- Incude the banner's CSS --->
  <link href = "/css/banner.css" rel = "stylesheet"/>
</head>
<body id = "body">
<div id = "banner">
  <a href = "/userHome">
    <img id = "QuickBitesLogo" src = "/pictures/QuickBites.png" object-fit = "contain" />
  </a>
  <div id = "bannerButtonContainer">
    <a href = "/vendorLogin">
      <button id = "vendorButton"> Looking to Sell? </button>
    </a>
  </div>
  <div id = "userButtonContainer">
    <div id = "shoppingCartIcon" hover class = "dropdown">
      <img id = "shoppingCart" src = "/pictures/shoppingcart.png" hover object-fit = "contain" >
      <div class="dropdown-content">
        <h2>
          Shopping Cart
        </h2>
        <table id = "shoppingCartTable" class = "itemListContainer">
          <tr>
            <th> Name </th>
            <th> Quantity </th>
            <th> Price </th>
          </tr>
        </table>
      </div>
    </div>
    <a href = "/userAccountInfo">
      <div id = "userAccountIcon" hover>
        <img id = "userAccount" src = "/pictures/userIcon.png" object-fit = "contain">
      </div>
    </a>
  </div>
</div>
